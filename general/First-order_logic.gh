        applyBiconditionalTransitivity

        (∀ x φ) (¬ (∀ x (¬ ψ))) ImplicationDisjunction swapBiconditional
        applyBiconditionalTransitivity
# Applying ThereExists we are done.
        applyBiconditionalTransitivity

        x ψ ThereExists
        (∀ x φ) buildImplicationAntecedent
        swapBiconditional
        applyBiconditionalTransitivity
