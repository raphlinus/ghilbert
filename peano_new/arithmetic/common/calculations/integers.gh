thm (EqReplaceEq1Add0Add0 () (
     replacee (= A (+ (+ B D) A'))
     substitution (= B C)) 
     (= A (+ (+ C D) A'))
  replacee substitution
  D addeq1i
  EqReplaceEq1Add0
)


## <title> Substitution </title>
## <table>
##   (= A (+ (+ (*  [ B ]   ]   ]  D) A') B'))
##   (=             [ B ]   [ C ] )
##   (= A (+ (+ (*  [   [   [ C ]  D) A') B'))
## </table>
thm (EqReplaceEq1Add0Add0Mul0 () (
     replacee (= A (+ (+ (* B D) A') B'))
     substitution (= B C)) 
     (= A (+ (+ (* C D) A') B'))
  replacee substitution
  D muleq1i
  EqReplaceEq1Add0Add0
)

