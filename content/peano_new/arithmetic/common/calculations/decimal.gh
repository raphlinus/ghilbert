## <title> Definition of One-Tenth </title>
defthm (df-.1 nat (.1) () () (= (.1) (/ (1) (10)))
  (/ (1) (10)) eqid
)



## <title> Substitution </title>
## <table>
##   (= A (* B  [ C ]   ]   ] ))
##   (=         [ C ]   [ D ] )
##   (= A (* B  [   [   [ D ] ))
## </table>
thm (EqReplaceEq1Mul1 () (
     replacee (= A (* B C))
     substitution (= C D)) 
     (= A (* B D))
  replacee substitution
  B muleq2i
  EqReplaceEq1
)

## <title> Convert Fraction to Decimal </title>
thm (fracToDecimal () () (= (/ A (10)) (* A (.1)))
  10notEqual0
  A expandFraci
  df-.1 eqcomi  
  EqReplaceEq1Mul1
)

